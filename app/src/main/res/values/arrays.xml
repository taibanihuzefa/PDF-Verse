        <item>@drawable/ic_rename_icon</item>
        <item>@drawable/ic_delete_icon</item>
        <item>@drawable/ic_reorder_icon</item>
        <item>@drawable/ic_watermark_icon</item>
        <item>@drawable/ic_lock_icon</item>
        <item>@drawable/ic_split_icon</item>
